plugin 'cocoapods-acknowledgements', :settings_bundle => true, :add_to_target => false

  target 'CetaceaSharedFramework-iOS' do
    use_frameworks!
    platform :ios, '10.0'
    project 'CetaceaSharedFramework/CetaceaSharedFramework.xcodeproj'
    workspace 'Cetacea'
    pod 'Realm'
    pod 'ReactiveObjC'
    pod 'DateTools'
    pod 'CocoaMarkdown', :git => 'https://github.com/SecondRenaissance/CocoaMarkdown.git'
  end

  target 'CetaceaSharedFramework-OSX' do
    use_frameworks!
    platform :osx, '10.12'
    project 'CetaceaSharedFramework/CetaceaSharedFramework.xcodeproj'
    workspace 'Cetacea'
    pod 'Realm'
    pod 'ReactiveObjC'
    pod 'DateTools'
    pod 'CocoaMarkdown', :git => 'https://github.com/SecondRenaissance/CocoaMarkdown.git'
  end

