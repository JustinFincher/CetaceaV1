    use_frameworks!
    platform :watchos, '3.0'
    project 'CetaceaSharedFramework/CetaceaSharedFramework.xcodeproj'
    workspace 'Cetacea'
  end

  target 'Cetacea-iOS' do
    use_frameworks!
    platform :ios, '10.0'
    project 'Cetacea-iOS/Cetacea-iOS.xcodeproj'
    workspace 'Cetacea'
    pod 'CocoaMarkdown', :git => 'https://github.com/SecondRenaissance/CocoaMarkdown.git'
    pod 'Realm'
    pod 'IQKeyboardManager'
    pod 'GDPerformanceView', '~> 1.3.1'
    pod 'PeerTalk', :git => 'https://github.com/rsms/peertalk.git'
    pod 'DateTools'
    pod 'MMMarkdown'
    pod 'Fabric'
    pod 'Crashlytics'
    pod 'ReactiveObjC'
    pod 'DynamicColor'
    pod 'SDVersion'
    pod 'DKNightVersion'
    pod 'Bohr', :git => 'https://github.com/SecondRenaissance/Bohr.git'
    pod 'SyntaxKit', :git => 'https://github.com/SecondRenaissance/SyntaxKit.git'
  end

  target 'WatchKitApp' do
    platform :watchos, '3.0'
    project 'Cetacea-iOS/Cetacea-iOS.xcodeproj'
    workspace 'Cetacea'
    pod 'SDVersion'
  end

  target 'WatchKitApp Extension' do
